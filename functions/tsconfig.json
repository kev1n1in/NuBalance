{
  "compilerOptions": {
    "module": "ESNext",  // 確保這裡是 ESNext 或 ES6
    "target": "ES6",
    "moduleResolution": "node",
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib"]
}
